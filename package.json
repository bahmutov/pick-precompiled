{
  "name": "pick-precompiled",
  "description": "Picks precompiled bundle during install",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "condition-node-version": "1.2.0",
    "semantic-release": "^4.3.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "echo \"Picking pre-compiled bundle\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/pick-precompiled.git"
  },
  "release": {
    "verifyConditions": {
      "path": "condition-node-version",
      "node": "4.2.2",
      "verbose": true
    }
  },
  "keywords": [
    "pre-compiled",
    "bundle",
    "ES6",
    "ES2015"
  ],
  "config": {
    "pre-compiled": {
      "dir": "test-dist",
      "files": [
        "test/main.js",
        "test/bin-main.js"
      ]
    }
  },
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/pick-precompiled/issues"
  },
  "homepage": "https://github.com/bahmutov/pick-precompiled#readme"
}
